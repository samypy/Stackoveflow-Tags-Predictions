Sub LookupAccountName()
    
    Dim accountRange As Range ' Range containing Account ID and Name
    Dim accountArray As Variant ' Array to hold Account ID and Name
    Dim accountDict As Object ' Dictionary to hold Account ID and Name as key-value pairs
    Dim accountId As Variant ' Account ID to lookup in the dictionary
    Dim accountName As Variant ' Account Name retrieved from the dictionary
    
    ' Set the range containing the Account ID and Name
    Set accountRange = ThisWorkbook.Sheets("Sheet1").Range("A2:B10")
    
    ' Convert the range to an array
    accountArray = accountRange.Value
    
    ' Create a dictionary to hold the Account ID and Name as key-value pairs
    Set accountDict = CreateObject("Scripting.Dictionary")
    
    ' Loop through the array and add the Account ID and Name to the dictionary
    Dim i As Long
    For i = LBound(accountArray, 1) To UBound(accountArray, 1)
        accountDict.Add accountArray(i, 1), accountArray(i, 2)
    Next i
    
    ' Lookup the Account Name using the Account ID in the dictionary
    accountId = "1234" ' Replace with the Account ID to lookup
    If accountDict.exists(accountId) Then
        accountName = accountDict(accountId)
        Debug.Print "Account Name: " & accountName
    Else
        Debug.Print "Account ID not found"
    End If
    
End Sub
