import pandas as pd

# Assuming you have a DataFrame called 'df' with the columns mentioned

# Create a list of month columns (excluding the first two columns: Year and Accountname)
month_columns = df.columns[2:]

# Calculate the rolling average for each month
for month in month_columns:
    df[month + ' Rolling Average'] = df[month].rolling(window=6, min_periods=1).mean()

# Print the updated DataFrame
print(df)
